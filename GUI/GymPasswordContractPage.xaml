<Page x:Class="GUI.GymPasswordContractPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GymPasswordContractPage">

    <Grid>

        <TextBox x:Name="Username" HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="200" Height="41" Margin="0,112,0,0"/>
        <TextBox x:Name ="Password" HorizontalAlignment="Center" Margin="0,267,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="200" Height="38"/>
        <Label Content="Benutzername:" HorizontalAlignment="Left" Margin="116,112,0,0" VerticalAlignment="Top" Height="43" Width="94"/>
        <Label Content="Passwort:" HorizontalAlignment="Left" Margin="116,262,0,0" VerticalAlignment="Top" Height="43" Width="94"/>
        <Button Content="Anmelden" HorizontalAlignment="Center" Margin="0,366,0,0" VerticalAlignment="Top" Height="59" Width="116" Click="LogIn_Click"/>

    </Grid>
</Page>
