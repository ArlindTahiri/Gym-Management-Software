<Page x:Class="GUI.GymHomepage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GymHomepage">

    <Grid>
        <Button Content="Mitglieder" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="61" Width="122" Click="Member_Click"/>
        <Button x:Name="EmployeeButton" Content="Mitarbeiter" HorizontalAlignment="Left" Margin="231,10,0,0" VerticalAlignment="Top" Height="61" Width="122" Click="EmployeeButton_Click"/>
        <Button Content="Nachrichten" HorizontalAlignment="Left" Margin="668,10,0,0" VerticalAlignment="Top" Height="61" Width="122"/>
        <Button x:Name="ContractButton" Content="Verträge" HorizontalAlignment="Left" Margin="441,10,0,0" VerticalAlignment="Top" Height="61" Width="122" Click="Contract_Click"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,203,367,10"/>
        <Button x:Name="Inventar" Content="Inventar" HorizontalAlignment="Left" Height="72" Margin="668,95,0,0" VerticalAlignment="Top" Width="122" Click="Inventar_Click"/>
        <Button x:Name="OrderButton" Content="Bestellungen" HorizontalAlignment="Left" Height="72" Margin="441,95,0,0" VerticalAlignment="Top" Width="122" Click="OrderButton_Click"/>

    </Grid>
</Page>
