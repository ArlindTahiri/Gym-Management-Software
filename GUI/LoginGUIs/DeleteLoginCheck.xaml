      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GUI.LoginGUIs"
      mc:Ignorable="d" 
      d:DesignHeight="250" d:DesignWidth="400"
      Title="DeleteLoginCheck">

    <Grid>
        <Label Content="Geben sie bitte den Login ein, welchen Sie löschen wollen" HorizontalAlignment="Center" Height="34" Margin="0,35,0,0" VerticalAlignment="Top" Width="353"/>
        <TextBox x:Name="LoginCheck" HorizontalAlignment="Center" Height="32" Margin="0,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="344"/>
        
    </Grid>
</Page>
