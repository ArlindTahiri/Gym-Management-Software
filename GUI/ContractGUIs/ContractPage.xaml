<Page x:Class="GUI.ContractGUIs.ContractPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GUI.ContractGUIs"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ContractPage">

    <Grid>
        <Button x:Name="EditContractButton" Content="Vertrag Bearbeiten" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="104" Width="249" Click="EditContractButton_Click"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,180,0,0"/>

    </Grid>
</Page>
