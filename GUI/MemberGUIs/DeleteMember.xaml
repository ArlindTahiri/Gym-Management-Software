<Page x:Class="GUI.MemberGUIs.DeleteMember"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GUI.MemberGUIs"
      mc:Ignorable="d" 
      d:DesignHeight="250" d:DesignWidth="250"
      Title="DeleteMember">

    <Grid>
        <Label Content="Wollen Sie das Mitglied wirklich löschen?" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="72"/>
        <Button x:Name="YesButton" Content="Ja" HorizontalAlignment="Left" Margin="10,105,0,0" VerticalAlignment="Top" Height="53" Width="81" Click="YesButton_Click"/>
        <Button x:Name="NoButton" Content="Nein" HorizontalAlignment="Left" Margin="145,105,0,0" VerticalAlignment="Top" Height="53" Width="81" Click="NoButton_Click"/>
        <Label x:Name="TestLabel" Content="" HorizontalAlignment="Left" Margin="45,197,0,0" VerticalAlignment="Top" Width="176"/>

    </Grid>
</Page>
